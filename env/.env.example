# 环境变量
# 标题
IHRM_APP_TITLE=IHRM
# 接口地址
IHRM_APP_API_BASE_URL=https://heimahr.itheima.net
# 基础路径
IHRM_BASE_URL=/sass-ihrm/
# 地图API
IHRM_MAP_API_KEY=XXXXXXXXXXXXXXXXXXXXXXXXXXX
# 描述
IHRM_APP_DESCRIPTION=企业级人力资源管理系统

# 这些放到env.development 和 env.production 文件中

# 是否开启 sourcemap (生产环境关闭以减小包体积)
IHRM_SOURCEMAP=false

# 是否开启 css 代码分割 (生产环境开启以优化加载性能)
IHRM_CSS_CODE_SPLIT=true

# 压缩配置 (生产环境开启压缩以减小包体积)
IHRM_MINIFY=true

# 是否开启 console (生产环境关闭以减小包体积和避免信息泄露)
IHRM_CONSOLE=false

# 是否开启 tree-shaking (生产环境开启以优化包体积)
IHRM_TREE_SHAKING=true


# 压缩配置
IHRM_MINIFY=true

# 是否开启产物分析报告
IHRM_BUILD_ANALYZE=true
